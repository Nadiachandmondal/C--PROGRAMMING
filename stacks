//stacks in c++

#include<iostream>
#include<stack>
using namespace std;
int main()
{
    //creation of stack
    
    stack <int> s ;

//push operation
    s.push(9);
    s.push(4);

    //pop operation
    s.pop();

    cout <<"printing top element "<< s.top()<<endl;

//empty operation
if(s.empty()){
    cout<<"stack is empty"<<endl;
}
else{
    cout<<"stack is not empty"<<endl; 
}

cout << "stack size is "<<s.size()<<endl;
    return 0;

}
